    </description>
    <version>dev.build</version>
    <vendor>Daniel Worthington-Bodart</vendor>

    <idea-version since-build="139.659.2"/>

    <extensions defaultExtensionNs="com.intellij">
        <java.compiler implementation="com.googlecode.jcompilo.intellij.JCompiloBackendCompiler"/>
        <compileServer.plugin classpath="asm-all-5.0.3.jar;shavenmaven-95-dep.jar;totallylazy-1.21.jar;yadic-1.5.jar"/>
    </extensions>
</idea-plugin>